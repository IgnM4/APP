<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <!-- Minimal components and placeholders for harvested files -->
  <Fragment>
    <DirectoryRef Id="INSTALLDIR">
      <Component Id="Cmp.NssmExe" Guid="*">
        <File Id="Fil.nssm.exe" Source="$(var.SourceDir)\third_party\nssm\nssm.exe" KeyPath="yes" />
      </Component>
    </DirectoryRef>
  </Fragment>

  <Fragment>
    <ComponentGroup Id="CG.Api">
      <ComponentRef Id="Cmp.NssmExe" />
      <!-- Components harvested into CG.Api will be referenced here -->
    </ComponentGroup>
  </Fragment>

  <Fragment>
    <ComponentGroup Id="CG.Desktop">
      <!-- Components harvested into CG.Desktop will be referenced here -->
    </ComponentGroup>
  </Fragment>

  <Fragment>
    <ComponentGroup Id="CG.OracleClient">
      <!-- Components harvested into CG.OracleClient will be referenced here -->
    </ComponentGroup>
  </Fragment>
</Wix>
